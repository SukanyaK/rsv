<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="sathish.app.model.SalesDetails" table="SALES_DETAILS">
		<meta attribute="class-description">
			This class contains the Product details.
		</meta>
		<id name="salesId" type="int" column="SALES_ID">
			<generator class="native" />
		</id>
		<property name="productDescription" column="SALES_PRODUCT_DESCRIPTION"/>
		<property name="quantity" column="SALES_QUANTITY" />
		<property name="unitRate" column="SALES_UNIT_RATE" />
		<property name="amount" column="SALES_AMOUNT" />
		<property name="salesEntryId" column="SALES_ENTRY_ID" />
	</class>
</hibernate-mapping>
